    "0003": {
        "esp":{
            "Tarot la Vega":{
                "a3":{
                    "text":"El encargo consiste en buscar inspiración en la Vega de Santiago de Chile, utilizando el entorno para crear un tarot con técnica y estética libre."
                },
                "a2" : {
                    "img" : "https://drive.google.com/uc?export=download&id=1pWqe7F8nmUuv00GffGznAgZUSuig2Kdu"
